#pragma kernel Test

RWStructuredBuffer<float> FloatsIN;
RWStructuredBuffer<float> FloatsOUT;

// indexing for rendering arrays
int idIndexing(uint3 id, int size) {
	return id.x + (size* id.y) + (size*size*id.z);
}

[numthreads(8,8,8)]
void Test (uint3 id : SV_DispatchThreadID)
{
	int index = idIndexing(id, 16);
	FloatsOUT[index] = FloatsIN[index];
}
